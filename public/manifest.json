{
  "manifest_version": 3,
  "name": "iTab - Chrome New Tab",
  "description": "Replace the new tab page with something awesome!",
  "version": "1.0.1",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "/extension-icon.png",
    "48": "/extension-icon.png",
    "128": "/extension-icon.png"
  }
}
